{
  "name": "sample-lib",
  "version": "0.0.0-SNAPSHOT.0",
  "description": "sample project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/ogis-nakagawa/sample-lib.git",
  "author": "Michio Nakagawa <Nakagawa_Michio@ogis-ri.co.jp>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "tsc",
    "start": "npm-run-all tsc && node .",
    "build": "npm-run-all clean tsc",
    "test": "jest",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:watch": "ts-node-dev --respawn --poll -r tsconfig-paths/register src/index.ts",
    "lint": "eslint 'src/**/*.[jt]s?(x)'",
    "lint:fix": "eslint 'src/**/*.[jt]s?(x)' --fix",
    "doc": "typedoc --out ./dist/docs/ ./src/"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.4.0",
    "tsconfig-paths": "^3.11.0",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  }
}
